{
  "org.apache.sling.hc.core.impl.servlet.HealthCheckExecutorServlet": {
    "jcr:primaryType":"sling:OsgiConfig",
    "servletPath": "/system/health",
    "disabled": false,
    "cors.accessControlAllowOrigin": "localhost"
  },
  "org.apache.felix.http.sslfilter.SslFilter": {
    "jcr:primaryType":"sling:OsgiConfig",
    "ssl-forward.header": "X-Forwarded-Proto",
    "ssl-forward.value": "https",
    "ssl-forward-cert.header": "X-Forwarded-SSL-Certificate",
    "rewrite.absolute.urls": "false"
  },
  "org.apache.sling.rewriter.impl.components.HtmlSerializerFactory": {
    "jcr:primaryType":"sling:OsgiConfig",
    "doctype-default": "xhtml5"
  },
  "org.apache.sling.jcr.resource.internal.JcrResourceResolverFactoryImpl": {
    "jcr:primaryType":"sling:OsgiConfig",
    "resource.resolver.mapping": ["/:/", "(?!/content/wp/)/content/[a-zA-Z0-9]+/:/", "/content/:/", "/system/docroot/:/", "/content/wp/index.html>/index2.html"]
  },
  "org.apache.sling.commons.log.LogManager": {
    "jcr:primaryType":"sling:OsgiConfig",
    "org.apache.sling.commons.log.file": "",
    "org.apache.sling.commons.log.level": "info"
  },
  "org.apache.sling.commons.log.LogManager.factory.writer-wp": {
    "jcr:primaryType":"sling:OsgiConfig",
    "org.apache.sling.commons.log.file": ""
  },
  "org.apache.sling.jcr.base.internal.LoginAdminWhitelist.fragment-wp": {
    "jcr:primaryType":"sling:OsgiConfig",
    "whitelist.name": "whistlepost",
    "whitelist.bundles": ["org.apache.sling.rewriter"]
  },
  "org.apache.sling.fsprovider.internal.FsResourceProvider-apps": {
    "jcr:primaryType": "sling:OsgiConfig",
    "provider.file": "/wp/apps",
    "provider.root": "/apps",
    "provider.fs.mode": "FILES_FOLDERS",
    "provider.initial.content.import.options": "overwrite",
    "provider.checkinterval": "1000",
    "provider.cache.size": "10000"
  },
  "org.apache.sling.fsprovider.internal.FsResourceProvider-content": {
    "jcr:primaryType": "sling:OsgiConfig",
    "provider.file": "/wp/content",
    "provider.root": "/content",
    "provider.fs.mode": "INITIAL_CONTENT",
    "provider.initial.content.import.options": "overwrite",
    "provider.checkinterval": "1000",
    "provider.cache.size": "10000"
  }
}
